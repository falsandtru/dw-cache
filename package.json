{
  "name": "dw-cache",
  "version": "0.0.35",
  "description": "Dual window cache adaptively coordinates the ratio of LRU to LFU using the two sliding windows.",
  "private": true,
  "homepage": "https://github.com/falsandtru/dw-cache",
  "repository": {
    "type": "git",
    "url": "https://github.com/falsandtru/dw-cache"
  },
  "keywords": [
    "cache",
    "lru",
    "lfu",
    "adaptive"
  ],
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "index.*",
    "src",
    "test",
    "*.ts",
    "*.js",
    "*.json",
    "*.map",
    "*.md",
    "NOTICE",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/lru-cache": "7.10.9",
    "@types/mocha": "9.1.1",
    "@types/power-assert": "1.5.8",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-unassert": "^3.2.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-plugin-redos": "^4.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "glob": "^8.0.3",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-power-assert": "^1.0.0",
    "lru-cache": "^7.10.1",
    "mocha": "^10.0.0",
    "npm-check-updates": "^13.1.1",
    "spica": "0.0.563",
    "ts-loader": "^9.3.0",
    "typescript": "4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "update": "ncu -u && npm i --no-shrinkwrap",
    "dev": "concurrently \"webpack --env mode=test -w\" \"karma start --auto-watch\"",
    "test": "webpack --env mode=test && karma start --single-run --concurrency 1 --reporters dots,coverage",
    "lint": "webpack --env mode=lint",
    "bench": "webpack --env mode=bench && karma start --concurrency 1 --browsers Chrome",
    "dist": "webpack --env mode=dist"
  },
  "author": "falsandtru",
  "license": "(Apache-2.0 AND MPL-2.0)"
}
